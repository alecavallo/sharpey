{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "prettier",
    "plugin:@typescript-eslint/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "react/prototypes": 0,
    "no-unused-vars": 1,
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1,
    "jsx-a11y/label-has-for": 0,
    "no-console": "warn"
  },
  "plugins": ["react", "import", "jsx-a11y"]
}
